<div class="row app-advert-show-info mt-md-3">
    {% if advert.surface %}
        <div class="col-6 col-md-4 font-weight-stone-500 text-center mb-2 mb-md-3">
            <div class="text-primary">Surface</div>
            <div class="grey-text">{{ advert.surface }} m²</div>
        </div>
